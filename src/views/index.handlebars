<h2>Productos</h2>

{{#each products}}
    <div>
        <h2>{{this.title}}</h2>
        <p>Description: {{this.description}}</p>
        <p>Code: {{this.code}}</p>
        <p>Price: {{this.price}}</p>
        <p>Status: {{#if this.status}}Available{{else}}Not Available{{/if}}</p>
        <p>Stock: {{this.stock}}</p>
        <p>Category: {{this.category}}</p>
        {{!-- <p>ID: {{this._id}}</p> --}}
    </div>
{{/each}}

